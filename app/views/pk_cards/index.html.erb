<p id="notice"><%= notice %></p>

<h1>Pk Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pk_cards.each do |pk_card| %>
      <tr>
        <td><%= link_to 'Show', pk_card %></td>
        <td><%= link_to 'Edit', edit_pk_card_path(pk_card) %></td>
        <td><%= link_to 'Destroy', pk_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pk Card', new_pk_card_path %>
